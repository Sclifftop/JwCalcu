/**calLog.wxss**/

page{
    background: #DACBBB;
}

.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
   color: #727974;
}

.clear {
  position: fixed;
  top: 90%;
  left:2%;
  cursor: pointer;
  width: 88px;
  height: 35px;
  background: #1db0b8;
  border-radius: 3px;
  box-shadow: 0px 4px rgba(0, 0, 0, 0.2);
  margin: 0;
  color: #00343f;
  line-height: 36px;
  text-align: center;
  user-select: none;
  transition: all 0.2s ease;
}

view.clear:active {
  box-shadow: 0px 0px #4d9c81;
  top: 91%;
}